{
	"name": "@rbxts/commander",
	"version": "0.3.2",
	"description": "A flexible command framework built for roblox-ts",
	"main": "out/init.lua",
	"scripts": {
		"prepack": "npm run compile && find out -name __tests__ -type d -prune -exec rm -rf {} \\;",
		"compile": "rm -rf out && rbxtsc --verbose",
		"postcompile": "echo \"return {}\" > out/client/interface/app/config.lua",
		"watch": "rbxtsc -w --type=game -p test -i test/include",
		"serve": "rojo serve test"
	},
	"keywords": [
		"roblox-ts",
		"rbxts",
		"command"
	],
	"author": "paradoxuum",
	"license": "MIT",
	"types": "out/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/paradoxuum/commander.git"
	},
	"bugs": {
		"url": "https://github.com/paradoxuum/commander/issues"
	},
	"files": [
		"out",
		"!**/*.tsbuildinfo"
	],
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"@rbxts/compiler-types": "2.3.0-types.1",
		"@rbxts/jest": "^0.1.0",
		"@rbxts/jest-globals": "^0.1.0",
		"@rbxts/types": "^1.0.779",
		"roblox-ts": "2.3.0-dev-4a3b6b9",
		"typescript": "~5.4"
	},
	"dependencies": {
		"@rbxts/beacon": "^2.1.1",
		"@rbxts/pretty-react-hooks": "^0.5.0",
		"@rbxts/react": "^0.3.3",
		"@rbxts/react-reflex": "^0.3.4",
		"@rbxts/react-roblox": "^0.3.6",
		"@rbxts/reflex": "^4.3.1",
		"@rbxts/ripple": "^0.8.1",
		"@rbxts/services": "^1.5.4",
		"@rbxts/t": "^3.1.1"
	}
}
