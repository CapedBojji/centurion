{
    "name": "@rbxts/centurion-ui",
    "description": "Terminal UI for the Centurion command framework",
    "version": "1.0.0",
    "author": "paradoxuum",
    "license": "MIT",
    "main": "out/init.lua",
    "types": "out/index.d.ts",
    "files": [
        "out",
        "!**/*.tsbuildinfo",
        "!**/__tests__"
    ],
    "keywords": [
        "roblox",
        "roblox-ts",
        "command"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/paradoxuum/centurion.git"
    },
    "bugs": {
        "url": "https://github.com/paradoxuum/centurion/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prepack": "yarn build",
        "build": "shx rm -rf out && rbxtsc --verbose",
        "dev": "shx rm -rf out && rbxtsc -w"
    },
    "devDependencies": {
        "@rbxts/centurion": "workspace:^",
        "@rbxts/compiler-types": "2.3.0-types.2",
        "@rbxts/types": "^1.0.799",
        "roblox-ts": "2.3.0-dev-26ec859",
        "shx": "^0.3.4",
        "typescript": "~5.5.4"
    },
    "dependencies": {
        "@rbxts/charm": "^0.6.0",
        "@rbxts/services": "^1.5.4",
        "@rbxts/set-timeout": "^1.1.2",
        "@rbxts/vide": "^0.4.4"
    },
    "peerDependencies": {
        "@rbxts/centurion": "workspace:^"
    }
}
