{
	"name": "centurion",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$className": "Folder",

						"centurion": {
							"$className": "Folder",
							"out": {
								"$path": "node_modules/@rbxts/centurion/out"
							},
							"node_modules": {
								"$className": "Folder",
								"@rbxts": {
									"$path": "node_modules/@rbxts/centurion/node_modules/@rbxts"
								}
							}
						},

						"centurion-ui": {
							"$className": "Folder",
							"out": {
								"$path": "node_modules/@rbxts/centurion-ui/out"
							},
							"node_modules": {
								"$className": "Folder",
								"@rbxts": {
									"$path": "node_modules/@rbxts/centurion-ui/node_modules/@rbxts"
								}
							}
						},

						"services": {
							"$path": "node_modules/@rbxts/services"
						}
					}
				}
			}
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",
			"server": {
				"$path": "out/server"
			}
		},

		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"client": {
					"$path": "out/client"
				}
			}
		},

		"TestService": {
			"$className": "TestService",
			"$properties": {
				"ExecuteWithStudioRun": true
			},
			"$path": "scripts"
		}
	}
}
